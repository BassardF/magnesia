{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": ".",
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }],
    "redirects":[{
        "source":"/maps",
        "destination":"/",
        "type":302
    }, {
        "source":"/map/**",
        "destination":"/",
        "type":302
    }]
  }
}
